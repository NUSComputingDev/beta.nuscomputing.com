<%= simple_form_for [:mc, :locker, @round] do |f| %>
	<%= f.input :start, as: :string, input_html: {class: 'datetimepicker', data: {date_time: ''}} %>
	<br>
	<%= f.input :end, as: :string, input_html: {class: 'datetimepicker', data: {date_time: ''}}  %>
	<br>
	<%= f.input :acad_year, collection: ['AY16/17', 'AY17/18', 'AY18/19', 'AY19/20', 'AY20/21'], include_blank: false %>
	<%= f.input :label, input_html: {value: f.object.label || 'Round 1'} %>
	<%= f.input :kind, collection: LockerRound.kinds.symbolize_keys.keys, include_blank: false %>
  <%= f.input :allocated, label: 'Allocation Performed?', as: 'radio_buttons' %>
	<%= f.button :submit, 'Submit', class: 'radius success' %>
  <%= link_to 'Cancel', :back, class: 'radius button alert' %>
<% end %>
